record(aSub, "$(P)CAL:RANGE") {
    field(DESC, "Finds the calibration range")
    
    field(INPA, "$(P)$(BDIR) MS CP")
    field(FTA, "STRING")

    field(INPB, "$(P)$(TDIR) MS CP")
    field(FTB, "STRING")
    
    field(OUTA, "$(P)$(HIGH_PV) PP MS")
    field(FTVA, "DOUBLE") 
   
    field(OUTB, "$(P)$(LOW_PV) PP MS")
    field(FTVB, "DOUBLE")

    field(SNAM, "find_calibration_range")
}
